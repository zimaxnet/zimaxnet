{
  "routes": [
    {
      "route": "/products/*",
      "serve": "/products/*"
    },
    {
      "route": "/products",
      "serve": "/products/index.html"
    },
    {
      "route": "/*",
      "serve": "/*"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*", "/css/*", "/js/*", "/api/*"]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
} 